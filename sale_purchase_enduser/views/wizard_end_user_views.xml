<odoo>
    <record id="view_end_user_details_wizard_form" model="ir.ui.view">
        <field name="name">end.user.details.wizard.form</field>
        <field name="model">end.user.details.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="end_user_detail"/>
                </group>
                <footer>
                    <button name="confirm_details" type="object" class="btn-primary" string="Save"/>
                    <button special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
