<odoo>
    <record id="sale_order_form_inherit_end_user" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.enduser</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_information']" position="inside">
                <group string="End User Information">
                    <field name="end_user_detail" widget="html"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
